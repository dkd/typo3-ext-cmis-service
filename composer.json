{
    "name": "dkd/typo3-ext-cmis-service",
    "type": "typo3-cms-extension",
    "description": "TYPO3 Extension CMIS Service",
    "keywords": ["cmis", "typo3"],
    "homepage": "https://phabricator.dkd.de/diffusion/EXTCMISS/",
    "license": "GPL-2.0",
    "autoload": {
        "psr-4": {
            "Dkd\\CmisService\\Tests\\": "Tests/",
            "Dkd\\CmisService\\": "Classes/"
        },
        "classmap": [
            "vendor/typo3/cms/typo3/sysext"
        ]
    },
    "replace": {
        "typo3/cms-composer-installers": "*"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "http://composer.typo3.org/"
        }
    ],
    "authors": [
        {
            "name": "Sascha Egerer",
            "homepage": "http://www.dkd.de",
            "role": "Developer"
        }
    ],
    "require": {
        "symfony/yaml": "~2.5.3"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "1.*",
        "phpunit/phpunit": "4.1.*",
        "mikey179/vfsStream": "~1.3.0",
        "typo3-ci/typo3sniffpool": "dev-master",
        "typo3-ci/typo3cms": "dev-master",
        "typo3/cms": "~6.2.4",
        "psr/log": "1.0.*@dev"
    }
}
