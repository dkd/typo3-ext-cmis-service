{
    "name": "dkd/cmis-service",
    "type": "typo3-cms-extension",
    "description": "TYPO3 Extension CMIS Service",
    "keywords": ["cmis", "typo3"],
    "homepage": "http://www.dkd.de/",
    "license": "GPL-2.0",
    "autoload": {
        "psr-4": {
            "Dkd\\CmisService\\": "Classes/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dkd\\CmisService\\Tests\\": "Tests/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "http://composer.typo3.org/"
        },
        {
            "url": "https://github.com/dkd/php-cmis-client.git",
            "type": "git"
        }
    ],
    "authors": [
        {
            "name": "Claus Due",
            "homepage": "http://www.dkd.de",
            "role": "Developer"
        },
        {
            "name": "Sascha Egerer",
            "homepage": "http://www.dkd.de",
            "role": "Developer"
        },
        {
            "name": "Johannes Goslar",
            "homepage": "http://www.dkd.de",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "typo3/cms": "^7.6|^8",
        "dkd/php-cmis": "~2.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "1.*",
        "phpunit/phpunit": "4.2.*",
        "mikey179/vfsStream": "~1.3.0",
        "typo3-ci/typo3sniffpool": "*",
        "typo3-ci/typo3cms": "*",
        "symfony/yaml": "~2.5.3",
        "psr/log": "1.0.*@dev"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    }
}
